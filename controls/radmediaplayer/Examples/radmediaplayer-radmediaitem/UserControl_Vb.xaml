<UserControl x:Class="Default_Vb"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
            
    
<!-- #region radmediaplayer-radmediaitem_0 -->
<telerik:RadMediaPlayer x:Name="telerik" IsChaptersPanelPinned="True"
FullScreenChanged="telerik_FullScreenChanged" IsVideoPanelPinned="True"
SelectedIndex="0">
     <telerik:RadMediaItem Source=http://ecn.channel9.msdn.com/o9/pdc09/wmv/CL16.wmv VideoStretch="None">
<telerik:RadMediaItem.Header>
<StackPanel Orientation="Horizontal">
<StackPanel Margin="8">
<TextBlock Text="Name: " FontWeight="Normal" />
<TextBlock Text="Optimizing for Performance with the Windows Performance Toolkit" Margin="4" FontWeight="Bold" />
</StackPanel>
<StackPanel Margin="8">
<TextBlock Text="Genre: " FontWeight="Normal" />
<TextBlock Text="Tech Video" Margin="4" FontWeight="Bold" />
</StackPanel>
</StackPanel>
</telerik:RadMediaItem.Header>
</telerik:RadMediaItem>
</telerik:RadMediaPlayer>
<!-- #endregion -->
</Grid>
</UserControl>
