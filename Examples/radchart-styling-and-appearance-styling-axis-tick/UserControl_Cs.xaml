<UserControl x:Class="WpfApplication1.Default_Cs"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
            
    
<!-- #region radchart-styling-and-appearance-styling-axis-tick_0 -->
<Grid x:Name="LayoutRoot"
      Background="White">
    ...
    <Line />
</Grid>
<!-- #endregion -->

<!-- #region radchart-styling-and-appearance-styling-axis-tick_1 -->
<Style x:Key="AxisXTickStyle"
       TargetType="Line">
    <Setter Property="Stroke"
            Value="Orange" />
</Style>
<Style x:Key="AxisYTickStyle"
       TargetType="Line">
    <Setter Property="Stroke"
            Value="Orange" />
</Style>
<!-- #endregion -->

<!-- #region radchart-styling-and-appearance-styling-axis-tick_4 -->
<Style x:Key="AxisXTickStyle"
       TargetType="Line">
    <Setter Property="Stroke"
            Value="Orange" />
</Style>
<Style x:Key="AxisYTickStyle"
       TargetType="Line">
    <Setter Property="Stroke"
            Value="Orange" />
</Style>
<!-- #endregion -->
</Grid>
</UserControl>
