<UserControl x:Class="Default_Vb"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
            
    
<!-- #region chart-customize-axis-appearance-through-style_0 -->
xmlns:control="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Charting" 
xmlns:chart="clr-namespace:Telerik.Windows.Controls.Charting;assembly=Telerik.Windows.Controls.Charting"
<!-- #endregion -->

<!-- #region chart-customize-axis-appearance-through-style_1 -->
<Grid x:Name="LayoutRoot" Background="White">
    <Grid.Resources>
        <Style x:Key="CustomAxisYStyle" TargetType="chart:Axis2D">
            <Setter Property="Stroke" Value="Red" />
            <Setter Property="StrokeThickness" Value="5" />
        </Style>
        <Style x:Key="CustomTickStyle" TargetType="chart:AxisItem2D">
            <Setter Property="Stroke" Value="Orange" />
            <Setter Property="StrokeThickness" Value="5" />
        </Style>
        <Style x:Key="CustomGridlineStyle" TargetType="chart:AxisItem2D">
            <Setter Property="Stroke" Value="Green" />
            <Setter Property="StrokeThickness" Value="5" />
        </Style>
        <Style x:Key="CustomStriplineStyle" TargetType="chart:StripLine2DBase">
            <Setter Property="Stroke" Value="Pink" />
            <Setter Property="StrokeThickness" Value="1" />
            <Setter Property="NormalFill" Value="DarkGray" />
            <Setter Property="AlternateFill" Value="Gray" />
        </Style>
        <Style x:Key="CustomItemLabelStyle" TargetType="TextBlock">
            <Setter Property="Foreground" Value="Pink" />
            <Setter Property="FontSize" Value="16" />
        </Style>
        <Style x:Key="CustomAxisTitleStyle" TargetType="chart:AxisTitle">
            <Setter Property="Foreground" Value="Pink" />
            <Setter Property="FontSize" Value="16" />
        </Style>
    </Grid.Resources>
    <control:RadChart x:Name="RadChart1" />
</Grid>
<!-- #endregion -->
</Grid>
</UserControl>
