<UserControl x:Class="WpfApplication1.Default_Cs"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
            
<!-- #region radtimeline_features_vertical_scrolling_0 -->
<telerik:RadTimeline ScrollMode="ScrollAndZoom"
                        PeriodStart="2011-01-01"
                        PeriodEnd="2012-01-01"
                        VisiblePeriodStart="2011-01-01"
                        VisiblePeriodEnd="2011-02-01"
                        VerticalScrollBarVisibility="Disabled"
                        StartPath="StartDate"
                        DurationPath="Duration"
                        ItemsSource="{Binding TimelineItems}">
    <telerik:RadTimeline.Intervals>
        <telerik:YearInterval />
        <telerik:MonthInterval />
        <telerik:WeekInterval />
        <telerik:DayInterval />
    </telerik:RadTimeline.Intervals>
</telerik:RadTimeline>
<!-- #endregion -->

<!-- #region radtimeline_features_vertical_scrolling_1 -->
<telerik:RadTimeline ScrollMode="ScrollOnly"
                        PeriodStart="2011-01-01"
                        PeriodEnd="2012-01-01"
                        VisiblePeriodStart="2011-01-01"
                        VisiblePeriodEnd="2011-02-01"
                        VerticalScrollBarVisibility="Auto"
                        StartPath="StartDate"
                        DurationPath="Duration"
                        ItemsSource="{Binding TimelineItems}">
    <telerik:RadTimeline.Intervals>
        <telerik:YearInterval />
        <telerik:MonthInterval />
        <telerik:WeekInterval />
        <telerik:DayInterval />
    </telerik:RadTimeline.Intervals>
</telerik:RadTimeline>
</GroupBox>
<!-- #endregion -->

<!-- #region radtimeline_features_vertical_scrolling_2 -->
<telerik:RadTimeline ScrollMode="ScrollOnly"
                        PeriodStart="2011-01-01"
                        PeriodEnd="2012-01-01"
                        VisiblePeriodStart="2011-01-01"
                        VisiblePeriodEnd="2011-02-01"
                        VerticalScrollBarVisibility="Auto"
                        StartPath="StartDate"
                        DurationPath="Duration"
                        ItemsSource="{Binding TimelineItems}">
    <telerik:RadTimeline.Intervals>
        <telerik:YearInterval />
        <telerik:MonthInterval />
        <telerik:WeekInterval />
        <telerik:DayInterval />
    </telerik:RadTimeline.Intervals>
</telerik:RadTimeline>
<!-- #endregion -->

<!-- #region radtimeline_features_vertical_scrolling_3 -->
<telerik:RadTimeline ScrollMode="None"
                        PeriodStart="2011-01-01"
                        PeriodEnd="2012-01-01"
                        VisiblePeriodStart="2011-01-01"
                        VisiblePeriodEnd="2011-02-01"
                        VerticalScrollBarVisibility="Hidden"
                        StartPath="StartDate"
                        DurationPath="Duration"
                        ItemsSource="{Binding TimelineItems}">
    <telerik:RadTimeline.Intervals>
        <telerik:YearInterval />
        <telerik:MonthInterval />
        <telerik:WeekInterval />
        <telerik:DayInterval />
    </telerik:RadTimeline.Intervals>
</telerik:RadTimeline>
<!-- #endregion -->

</Grid>
</UserControl>
